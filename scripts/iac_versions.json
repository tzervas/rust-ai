  "schema_resources": {
    "terraform": {
      "command": "terraform providers schema -json",
      "description": "Extract complete schema for all configured providers in JSON format",
      "output_structure": {
        "format": "Providers Schema Representation",
        "includes": [
          "Provider metadata",
          "Resources and data sources schemas",
          "Block representations with attributes",
          "Function signatures and documentation"
        ]
      }
    },
    "kubernetes": {
      "discovery_endpoints": {
        "openapi_v3_root": "/openapi/v3",
        "group_version_specific": "/openapi/v3/apis/<group>/<version>",
        "legacy_group": "/openapi/v3/api/v1"
      },
      "crd_validation": "CustomResourceDefinition uses openAPIV3Schema field for validation against JSON Schema"
    },
    "helm": {
      "values_schema": "values.schema.json",
      "location": "Must be in same directory as values.yaml",
      "format": "JSON Schema vocabulary",
      "validation_triggers": [
        "helm install",
        "helm upgrade",
        "helm template",
        "helm lint"
      ]
    },
    "docker_compose": {
      "schema_file": "JSON-schema for compose file format versions",
      "documentation": "https://docs.docker.com/reference/compose-file/",
      "versions_supported": "3.x and later"
    },
    "github_actions": {
      "workflow_location": ".github/workflows/",
      "file_extension": [".yml", ".yaml"],
      "schema_store_project": "schemastore.org",
      "parser": "YAML-based with JSON Schema validation"
    },
    "ansible": {
      "validation_tool": "ansible-lint",
      "schema_rule": "schema[playbook]",
      "validation_method": "JSON Schema against metadata files",
      "play_validation": "argument_specs in .meta.yml file"
    }
